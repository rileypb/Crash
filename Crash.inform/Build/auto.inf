	! Check if we are writing to the default map, and if so make a new map
	if (BlkValueGetLongBlock(map) == MAP_TY_Default_LB) {
		temp = MAP_TY_Create(mapkov);
		BlkValueCopy(map, temp);
		BlkValueFree(temp);
